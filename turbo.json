{
  "$schema": "https://turborepo.dev/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**", ".vitepress/dist/**"],
      "cache": true
    },
    "typecheck": {
      "outputs": []
    },
    "build-blocks": {
      "outputs": ["src/blocks/**"]
    },
    "docs:build": {
      "cache": true,
      "outputs": [".vitepress/dist/**"],
      "dependsOn": ["hikkaku-docs#build", "hikkaku#build"]
    }
  }
}
